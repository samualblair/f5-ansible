{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/main/schema/latest/as3-schema.json",
    "class": "AS3",
    "controls": {
        "trace": true
    },
    "declaration": {
        "class": "ADC",
{% if item.unit.as3_version is defined and item.unit.as3_version %}
        "schemaVersion": "{{ item.unit.as3_version }}",
{% else %}
        "schemaVersion": "3.55.0",
{% endif %}
{% if item.unit.generated_uuid is defined and item.unit.generated_uuid %}
        "id": "urn:uuid:{{ item.unit.generated_uuid }}",
{% endif %}
        "label": "Custom Generated Declaration",
        "remark": "Auto-generated by Ansible",
{% if item.unit.bigiq_as3_generate is defined and item.unit.bigiq_as3_generate == "yes" %}
{# 
Warning - As of 12-2023 Any AS3 version over 3.41.0 is not supported with BIG-IQ 
Warning - As of 9-2024 Any AS3 version over 3.53.0 are again supported with BIG-IQ 
#}
        "target": {
            "address": "{{ item.unit.oob_mgmt_ip }}"
        },
{% endif %}
        "Common": {
            "class": "Tenant",
            "Shared": {
                "class": "Application",
                "template": "shared",
                "{{ item.unit.irule1 }}": {
                    "class": "iRule",
                    "iRule": {
                        "base64": ""
                    }
                }
            }
        },
        "{{ item.unit.partition1 }}": {
            "class": "Tenant",
            "Shared": {
                "class": "Application",
                "template": "shared",
                "WHITELIST_IRULE_APP": {
                    "class": "iRule",
                    "iRule": {
                        "base64": ""
                    }
                }
            }
        }
    }
}