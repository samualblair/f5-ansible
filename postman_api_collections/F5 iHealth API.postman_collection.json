			"name": "Upload_qkview",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"('use strict');",
							"",
							"",
							"function autoSetF5iHealthLocations() {",
							"    'use strict';",
							"    const xml2js = require('xml2js');",
							"    let jsonResponse;",
							"",
							"    xml2js.parseString(pm.response.text(), { explicitArray: false }, (err, result) => {",
							"        jsonResponse = result;",
							"    });",
							"",
							"    pm.environment.set(\"api_diagnostics\", jsonResponse.qkview.diagnostics);",
							"    pm.environment.set(\"api_files\", jsonResponse.qkview.files);",
							"    pm.environment.set(\"api_commands\", jsonResponse.qkview.commands);",
							"    pm.environment.set(\"api_bigip\", jsonResponse.qkview.bigip);",
							"    pm.environment.set(\"gui_uri\", jsonResponse.qkview.gui_uri);",
							"",
							"};",
							"",
							"autoSetF5iHealthLocations();",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
